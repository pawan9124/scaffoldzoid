{
  "name": "scaffoldzoid",
  "version": "1.0.0",
  "description": "Bridge of the orange seller",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "export NODE_ENV=test && mocha --timeout 10000",
    "server": "PORT=8000 node server.js",
    "client": "PORT=3001 npm start --prefix client",
    "build": "npm run build --prefix client",
    "start": "export NODE_ENV=dev && node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client && npm install --dev --prefix client"
  },
  "author": "pawan saket",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cloudinary": "^1.24.0",
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.17",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^4.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.0",
    "chai-http": "^4.3.0",
    "faker": "^5.4.0",
    "mocha": "^8.3.0",
    "supertest": "^6.1.3",
    "supertest-as-promised": "^4.0.2"
  }
}
